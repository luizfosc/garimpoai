{
  "name": "@aios/garimpoai",
  "version": "0.1.0",
  "description": "GarimpoAI - Assistente pessoal de licitações públicas com IA conversacional",
  "main": "src/index.ts",
  "bin": {
    "garimpoai": "./bin/garimpoai.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "start": "tsx src/index.ts",
    "chat": "tsx src/index.ts chat",
    "collect": "tsx src/index.ts collect",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/database/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "licitacoes",
    "pncp",
    "governo",
    "compras-publicas",
    "ai",
    "conversational",
    "cli",
    "garimpo"
  ],
  "author": "AIOS Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "better-sqlite3": "^11.8.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "drizzle-orm": "^0.38.0",
    "js-yaml": "^4.1.0",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.66.0",
    "nodemailer": "^6.10.0",
    "ora": "^5.4.1",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@types/nodemailer": "^6.4.17",
    "drizzle-kit": "^0.30.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3"
  }
}
